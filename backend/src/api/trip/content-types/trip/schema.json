    },
    "Time": {
      "type": "string"
    },
    "Vendor_name": {
      "type": "string",
      "required": true
    },
    "Difficulties": {
      "type": "blocks"
    },
    "Exclusions": {
      "type": "blocks"
    },
    "Inclusion": {
